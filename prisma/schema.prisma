generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Molecula{
  id Int @default(autoincrement()) @unique
  smiles String @unique
  peso_molecular Float
  lipinski_ok Boolean
  prediccion_bioactiva Float
  toxicidad_potencial String
}